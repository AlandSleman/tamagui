{
  "expo": {
    "name": "belaytionship",
    "slug": "belaytionshiprocks",
    "scheme": "belaytionshiprocks",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/Belaytionship-180.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/6a9f45d5-4f31-4556-bd99-0d7d09bdebee"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.belaytionship.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.belaytionship.app",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "eas": {
          "projectId": "298b9397-940d-4706-a2db-a10029e2ca25"
        },
        "projectId": "6a9f45d5-4f31-4556-bd99-0d7d09bdebee"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
