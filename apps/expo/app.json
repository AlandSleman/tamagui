{
  "expo": {
    "name": "belaytionship",
    "slug": "expo-belaytionship",
    "scheme": "belaytionshiprocks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Belaytionship-180.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/f90e58a7-e2f9-408c-8655-8e00d2037f16"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.belay.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.belaytionship.app",
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "slug": "belaytionship",
        "projectId": "f90e58a7-e2f9-408c-8655-8e00d2037f16"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
