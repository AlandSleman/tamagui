{
  "expo": {
    "name": "belaytionship",
    "slug": "expo-belaytionship",
    "scheme": "belaytionshiprocks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Belaytionship-180.png",
    "userInterfaceStyle": "dark",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/f90e58a7-e2f9-408c-8655-8e00d2037f16"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.belay.app",
      "splash": {
        "image": "./assets/iphone-splash.png",
        "contentFit": "cover",
        // "contentFit": "contain",
        "backgroundColor": "#fff",
        "dark": {
          "image": "./assets/iphone-splash-dark.png",
          "contentFit": "cover",
          "backgroundColor": "#000"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0000"
      },
      "package": "com.belaytionship.app",
      "permissions": ["android.permission.RECORD_AUDIO"],
      "splash": {
        "image": "./assets/iphone-splash.png",
        "contentFit": "cover",
        "backgroundColor": "#000000",
        "dark": {
          "backgroundColor": "#000000",
          "image": "./assets/iphone-splash-dark.png"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/Belaytionship-180.png",
          "color": "#000000",
          "androidMode": "collapse",
          "androidCollapsedTitle": "Belaytionship",
          "androidCollapsedBody": "You have a new message from {{sender}}"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "slug": "belaytionship",
        "projectId": "f90e58a7-e2f9-408c-8655-8e00d2037f16"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
