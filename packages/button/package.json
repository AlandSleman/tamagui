{
  "name": "@tamagui/button",
  "version": "1.14.3",
  "sideEffects": [
    "*.css"
  ],
  "source": "src/index.ts",
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "module:jsx": "dist/jsx",
  "files": [
    "src",
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "lint": "../../node_modules/.bin/rome check src",
    "lint:fix": "../../node_modules/.bin/rome check --apply-suggested src"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/default.d.ts",
      "import": "./dist/esm/default.mjs",
      "require": "./dist/cjs/default.js"
    },
    "./headless": {
      "types": "./types/headless.d.ts",
      "import": "./dist/esm/headless.mjs",
      "require": "./dist/cjs/headless.js"
    }
  },
  "dependencies": {
    "@tamagui/font-size": "1.14.3",
    "@tamagui/get-button-sized": "1.14.3",
    "@tamagui/helpers-tamagui": "1.14.3",
    "@tamagui/text": "1.14.3",
    "@tamagui/web": "1.14.3"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@tamagui/build": "1.14.3",
    "react": "^18.2.0",
    "vitest": "^0.26.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "a49cc7ea6b93ba384e77a4880ae48ac4a5635c14"
}
