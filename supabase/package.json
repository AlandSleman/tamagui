{
  "name": "@my/supabase",
  "type": "module",
  "version": "0.0.0",
  "files": [
    "./types.ts"
  ],
  "scripts": {
    "_with-env": "dotenv -e ../.env -c -- ",
    "_link-project": "npx supabase link --project-ref slwmxbbwegsfgxepfjlg --workdir ../",
    "link-project": "yarn _with-env yarn _link-project",
    "_generate:remote": "npx supabase gen types typescript --project-id slwmxbbwegsfgxepfjlg --schema public > ./types.ts",
    "generate:remote": "yarn _with-env yarn _generate:remote",
    "generate": "npx supabase gen types typescript --local --schema public > ./types.ts",
    "g": "yarn generate",
    "build": "tsc",
    "migration:diff": "npx supabase db diff -f ",
    "start": "npx supabase start",
    "stop": "npx supabase stop",
    "_seeds": "node -r ts-node/register ./seeds.ts",
    "__seeds": "npx tsx ./seeds.ts",
    "reset": "npx supabase db reset && npx supabase start && yarn generate && yarn __seeds",
    "status": "npx supabase status",
    "_deploy": "yarn link-project && npx supabase db push",
    "deploy": "yarn _with-env yarn _deploy"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@supabase/supabase-js": "^2.33.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^6.0.0",
    "supabase": ">=1.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "rimraf": "^5.0.1"
  }
}
